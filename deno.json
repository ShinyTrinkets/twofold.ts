{
    "imports": {
        "shell-quote": "npm:shell-quote"
    },
    "lint": {
        "include": [
            "src/"
        ],
        "rules": {
            "tags": [
                "recommended"
            ],
            "exclude": [
                "ban-types",
                "no-explicit-any",
                "no-node-globals",
                "no-process-global",
                "no-unused-vars",
                "no-unversioned-import"
            ]
        }
    }
}